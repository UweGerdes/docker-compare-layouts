<% if (result) { %>
<tr class="<% if (result.compareImagesStderr != '0') { %>fail<% } else { %>success<% } %>">
	<td><% if (result.jsonFilename) { %><a href="/show/<%= name %>/<%= result.name %>/<%= result.viewport %>" title="Testergebnis"><%= result.name %> <%= result.viewport %></a><% } else { %><%= result.name %> <%= result.viewport %><% } %></td>
	<td><a href="/results/<%= config.destDir %>/<%= result.subdir1 %>/<%= result.viewport %>/page.png" title="Screenshot Seite" class="compare1"><%= result.page1.url %></a> <% if (result.exists1) { %><a href="/<%= result.baseFilename1 %>.png" title="Screenshot Element" class="compare1"><%= result.selector1 %></a><% } else { %><%= result.selector1 %> NOT FOUND<% } %></td>
	<td><a href="/results/<%= config.destDir %>/<%= result.subdir2 %>/<%= result.viewport %>/page.png" title="Screenshot Seite" class="compare2"><%= result.page2.url %></a> <% if (result.exists2) { %><a href="/<%= result.baseFilename2 %>.png" title="Screenshot Element" class="compare2"><%= result.selector2 %></a><% } else { %><%= result.selector2 %> NOT FOUND<% } %></td>
</tr>
<% } else { %>
<tr class="fail">
	<td><%= name %>: Fehler bei Bearbeitung</td>
	<td colspan="2">siehe ./results/<%= config.destDir %>/<%= name %></td>
</tr>
<% } %>
