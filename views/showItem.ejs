<% if (result) { %>
<tr class="<% if (result.imageStderr != '0') { %>fail<% } else { %>success<% } %>">
	<td><% if (result.jsonFilename) { %><a href="/show/<%= config.name %>/<%= result.width %>/<%= result.name %>" title="Testergebnis"><%= result.name %> <%= result.width %></a><% } else { %><%= result.name %> <%= result.width %><% } %></td>
	<td><a href="/results/<%= config.data.destDir %>/<%= result.width %>/<%= result.subdir1 %>/page.png" title="Screenshot Seite" class="compare1"><%= result.page1.url %></a> <% if (result.exists1) { %><a href="/<%= result.baseFilename1 %>.png" title="Screenshot Element" class="compare1"><%= result.selector1 %></a><% } else { %><%= result.selector1 %> NOT FOUND<% } %></td>
	<td><a href="/results/<%= config.data.destDir %>/<%= result.width %>/<%= result.subdir2 %>/page.png" title="Screenshot Seite" class="compare2"><%= result.page2.url %></a> <% if (result.exists2) { %><a href="/<%= result.baseFilename2 %>.png" title="Screenshot Element" class="compare2"><%= result.selector2 %></a><% } else { %><%= result.selector2 %> NOT FOUND<% } %></td>
</tr>
<% } else { %>
<tr class="fail">
	<td><%= config.name %>: Fehler bei Bearbeitung</td>
	<td colspan="2">siehe ./results/<%= config.data.destDir %>/<%= result.width %></td>
</tr>
<% } %>
